{
	"version": 2,
	"buildCommand": "pnpm run build",
	"builds": [
		{
			"src": "dist/app.js",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["dist/**"]
			}
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/dist/app.js"
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/(.*)",
			"status": 404,
			"dest": "/dist/app.js"
		}
	]
}
